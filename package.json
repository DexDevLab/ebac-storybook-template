{
  "name": "ebac-storybook-template",
  "description": "Template de Aplicação React com Storybook para exercício do Módulo 14 da Seção 'React: do Zero ao Pro' do curso 'Profissão: Engenheiro Front-End' da Instituição EBAC - Escola Britânica de Artes Criativas e Tecnologia",
  "version": "v0.2.0-240426",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:app": "yarn run start:react",
    "dev:express": "yarn run start:express",
    "dev:storybook": "yarn start:storybook",
    "start": "run-p start:*",
    "start:express": "node src/express.js",
    "start:react": "react-scripts start",
    "start:storybook": "storybook dev -p 6006",
    "build": "npm-run-all build:*",
    "build:react": "react-scripts build",
    "build:storybook": "storybook build",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --force-rebuild",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "npm-run-all2": "^6.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "serve": "^14.2.2",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@storybook/addon-docs": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/manager-api": "^8.0.8",
    "@storybook/preset-create-react-app": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-webpack5": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@storybook/theming": "^8.0.8",
    "chromatic": "^11.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "prop-types": "^15.8.1",
    "storybook": "^8.0.8",
    "storybook-dark-mode": "^4.0.1",
    "webpack": "^5.91.0"
  }
}
